{
  "name": "liveplay-code-challenge",
  "version": "1.0.0",
  "main": "index.js",
  "engines": {
    "node": "8.0.x"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bogdandorca/code-challenge.git"
  },
  "license": "MIT",
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
    },
    "snapshotSerializers": [
      "<rootDir>/node_modules/jest-serializer-vue"
    ]
  },
  "scripts": {
    "start": "./node_modules/parcel/bin/cli.js build index.html && node index.js",
    "postinstall": "./node_modules/parcel/bin/cli.js build index.html --public-url ./src",
    "lint": "eslint src",
    "dev": "parcel index.html",
    "test": "mocha specs/**/*-specs.js src/**/*-specs.js"
  },
  "devDependencies": {
    "@vue/component-compiler-utils": "^1.0.0",
    "@vue/server-test-utils": "^1.0.0-beta.15",
    "@vue/test-utils": "^1.0.0-beta.15",
    "babel-core": "^6.26.0",
    "babel-jest": "^22.4.3",
    "babel-loader": "^7.1.4",
    "eslint": "^3.19.0",
    "eslint-plugin-vue": "^4.5.0",
    "http-server": "^0.9.0",
    "jest": "^22.4.3",
    "jest-serializer-vue": "^1.0.0",
    "jsdom": "9.11.0",
    "jsdom-global": "2.1.1",
    "mocha": "^3.2.0",
    "node-sass": "^4.8.3",
    "parcel": "^1.8.1",
    "parcel-bundler": "^1.7.1",
    "parcel-plugin-eslint": "^1.0.4",
    "parcel-plugin-vue": "^1.5.0",
    "should": "^11.2.0",
    "vue-jest": "^2.5.0",
    "vue-template-compiler": "^2.5.16"
  },
  "dependencies": {
    "bootstrap": "^3.3.7",
    "express": "^4.16.3",
    "express-static": "^1.2.5",
    "vue": "^2.5.16",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  }
}
